{
  "name": "remark-table-to-code",
  "version": "0.2.0",
  "author": "David Worms <david@adaltas.com> (http://www.adaltas.com)",
  "dependencies": {
    "remark-gfm": "^3.0.1",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./lib/index.js"
      },
      "require": {
        "default": "./dist/remark-table-to-code.cjs"
      }
    },
    "./md_ast_to_string": {
      "import": {
        "default": "./lib/md_ast_to_string.js"
      }
    }
  },
  "devDependencies": {
    "dedent": "^0.7.0",
    "mocha": "^10.2.0",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "rollup": "^3.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "should": "^13.2.3"
  },
  "files": [
    "/lib"
  ],
  "license": "MIT",
  "main": "./dist/remark-table-to-code.cjs",
  "module": "lib/index.js",
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should"
    ],
    "inline-diffs": true,
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "build": "rollup -c",
    "prerelease": "npm run build",
    "test": "mocha 'test/**/*.js'"
  },
  "type": "module"
}
